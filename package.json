{
  "name": "metatavu-home-lambdas",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "lint": "eslint ./src --ext .ts",
    "postinstall": "npm run build-client && npm run build-severa",
    "build-client": "openapi-generator-cli generate -i time-bank-api-spec/swagger.yaml -o ./src/generated/client -c generator-config.json -g typescript-node",
    "build-severa": "openapi-generator-cli generate -i severa-API-spec/swagger.yaml -o ./src/generated/severaClient -c generator-config.json -g typescript-node"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@googleapis/docs": "^3.3.0",
    "@googleapis/drive": "^8.14.0",
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "@sinclair/typebox": "^0.33.12",
    "@slack/web-api": "^6.7.0",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "keycloak-js": "^26.0.2",
    "luxon": "^2.4.0",
    "moment": "^2.29.4",
    "node-fetch": "^2.7.0",
    "request": "^2.88.2",
    "serverless-dynamodb": "^0.2.53"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.0",
    "@types/node": "^14.18.63",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-jest": "^27.5.1",
    "coveralls": "^3.1.1",
    "esbuild": "^0.14.11",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "json-schema-to-ts": "^1.5.0",
    "keycloak-mock": "^1.0.10",
    "serverless": "^3.39.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^13.8.0",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "keywords": []
}
